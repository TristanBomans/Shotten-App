{
  "name": "shotten-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/generate-version.js && next build",
    "start": "next start",
    "lint": "eslint",
    "build:cf": "node scripts/generate-version.js && next build && opennextjs-cloudflare && node scripts/build-worker.js",
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 3000",
    "deploy": "opennextjs-cloudflare && cp .open-next/worker.js .open-next/assets/_worker.js && wrangler deploy"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.0",
    "@tailwindcss/postcss": "^4.1.17",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.556.0",
    "next": "14.2.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-tooltip": "^5.30.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.5.2",
    "@cloudflare/workers-types": "^4.20250109.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.22",
    "eslint": "^8",
    "eslint-config-next": "14.2.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5",
    "wrangler": "^3.107.2"
  }
}
